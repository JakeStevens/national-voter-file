alter table precinct_dim  alter column precinct_code TYPE VARCHAR(8) ;

alter table precinct_dim 
ADD COLUMN CAREER_CENTER VARCHAR(64) NULL,
ADD COLUMN CITY VARCHAR(32) NULL,
ADD COLUMN COUNTY_COURT_DISTRICT VARCHAR(3), 
ADD COLUMN COURT_OF_APPEALS VARCHAR(3), 
ADD COLUMN EDU_SERVICE_CENTER_DISTRICT VARCHAR(64), 
ADD COLUMN MUNICIPAL_COURT_DISTRICT VARCHAR(64), 
ADD COLUMN STATE_BOARD_OF_EDUCATION VARCHAR(3), 
ADD COLUMN TOWNSHIP  VARCHAR(32), 
ADD COLUMN VILLAGE VARCHAR(32), 
ADD COLUMN WARD VARCHAR(32); 


truncate table PRECINCT_DIM cascade;


alter table precinct_dim 
ALTER COLUMN CAREER_CENTER  TYPE VARCHAR(64) ,
ALTER COLUMN EDU_SERVICE_CENTER_DISTRICT  TYPE VARCHAR(64),
ALTER COLUMN MUNICIPAL_COURT_DISTRICT  TYPE VARCHAR(64);

